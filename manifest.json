{
    "name": "Sbat tabs",
    "description": "Reverse the order of all the tabs in your current window.",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Reverse tab order"
    },
    "icons": {
        "16": "icons/neg-sbat.png",
        "48": "icons/neg-sbat.png",
        "128": "icons/neg-sbat-128.png"
    },
    "permissions": ["tabs", "windows"],
    "background": {
        "scripts": ["sbat.js"],
        "persistent": false
    }
}
