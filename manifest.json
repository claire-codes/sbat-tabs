{
    "manifest_version": 2,

    "name": "Sbat tabs",
    "description": "Reverse the order of all the tabs in your current window",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Sbat us!"
    },
    "permissions": [
        "tabs",
        "windows"
    ],
    "background": {
        "scripts": ["sbat.js"],
        "persistent": false
    }
}